<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:top="2dp"
        android:left="2dp"
        android:right="2dp">
        <shape
            android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="@color/App_Main_Color_Green">
            </stroke>
            <solid
                android:color="#00FF0000">
            </solid>
            <corners
                android:radius="25dp">
            </corners>
        </shape>
    </item>
</layer-list>